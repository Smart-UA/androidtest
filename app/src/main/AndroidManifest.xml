<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="home.smart.cn.ua.androidtest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light">
        <activity
            android:name="home.smart.cn.ua.androidtest.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="home.smart.cn.ua.androidtest.AboutActivity" android:label="@string/app_name" ></activity>
        <activity android:name="home.smart.cn.ua.androidtest.LoginActivity" android:label="@string/app_name" ></activity>
        <activity android:name="home.smart.cn.ua.androidtest.RegisterActivity" android:label="@string/app_name" ></activity>
    </application>

</manifest>